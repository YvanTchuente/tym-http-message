{
    "name": "tym/http-message",
    "description": "The Smart HTTP message package.",
    "keywords": [
        "psr-7",
        "http",
        "message"
    ],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Yvan Tchuente",
            "email": "yvantchuente@gmail.com"
        }
    ],
    "require": {
        "php": "^8.0",
        "psr/http-message": "^1.0",
        "psr/http-factory": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    },
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "Tym\\Http\\Message\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    }
}